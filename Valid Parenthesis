#include<stdio.h>
#include<string.h>
#include<stdlib.h>
#include<math.h>
int main(){
    char str[100];
    gets(str);
    int len = strlen(str), flag=0, top=-1;
    char stack[len];
    for(int i=0;i<len;i++){
        char ch = str[i];
        if(ch == '(' || ch == '[' || ch == '{'){
            stack[++top]=ch;
        }
        else if(ch == ')' || ch ==']' || ch == '}'){
            if(ch == ')' && stack[top] == '('){
                top--;
            }
            else if(ch == ']' && stack[top] == '['){
                top--;
            }
            else if(ch == '}' && stack[top] == '{'){
                top--;
            }
            else{
                flag = 1;
                break;
            }
        }
    }
    if(flag == 0 && top == -1){
        printf("%s is Valid",str);
    }
    else{
        printf("%s is Invalid",str);
    }
    return 0;
}
