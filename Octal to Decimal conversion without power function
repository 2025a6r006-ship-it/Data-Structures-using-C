#include<stdio.h>
int main(){
    int num;
    printf("Enter Octal Number: ");
    scanf("%d",&num);
    int decNum=0, base=1;
    int temp = num;
    while(num!=0){
        int rem = num%10;
            decNum+= rem*base;
            base*=8;
            num/=10;
    }
    printf("Decimal of %d is %d", temp,decNum);

    return 0;
}
