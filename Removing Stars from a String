#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char S[100];
    scanf("%[^\n]",S);
    int len = strlen(S),top=-1;
    char stack[len];
    for(int i=0;i<len;i++){
        if(S[i]!='*'){
            stack[++top] = S[i];
        }
        else{
            top--;
        }
    }
    stack[++top] = '\0';
    printf("%s",stack);
    return 0;
}
